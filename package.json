{
  "name": "express-drizzle",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsup",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "db:generate": "drizzle-kit  generate:pg --config=drizzle.config.ts",
    "db:push": "drizzle-kit push:pg --config ./drizzle.config.ts",
    "db:drop": "drizzle-kit drop --config ./drizzle.config.ts",
    "db:introspect": "drizzle-kit introspect:pg --config=drizzle.config.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@neondatabase/serverless": "^0.8.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.3",
    "drizzle-orm": "^0.29.3",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.78.1",
    "pdf-parse": "^1.1.1",
    "socket.io": "^4.7.4",
    "tesseract.js": "^6.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.11.17",
    "@types/pdf-parse": "^1.1.4",
    "drizzle-kit": "^0.20.14",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsup": "^8.3.5",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "tsup": {
    "entry": [
      "src",
      "!src/migrations/*",
      "!src/uploads/*"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "noExternal": [
      "!**/*.sql"
    ],
    "external": [
      "**/*.sql"
    ]
  }
}
